<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rmd">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Timetable+"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeDark">
        <activity android:name=".GuideActivity"></activity>

        <service
            android:name=".myService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".bottomNavigation"
            android:label="@string/title_activity_bottom_navigation" />
        <activity android:name=".add_timetable" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".add" />
        <activity android:name=".GreetingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>